{
  "name": "neurohq",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node -e \"console.log('\\n=== NEUROHQ dev server – watch THIS terminal for compilation (✓ Compiled) and errors (⨯ Error) ===\\n')\" && next dev -p 3000",
    "start": "next dev -p 3000",
    "build": "next build",
    "deploy:check": "next build",
    "serve": "next start -p 3000",
    "serve:prod": "next start -p 3001",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "seed:quotes": "node scripts/seed-quotes.js",
    "build:icons": "node scripts/generate-pwa-icons.js",
    "generate-vapid": "node scripts/generate-vapid-keys.js",
    "db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > types/database.types.ts",
    "postinstall": "node scripts/patch-three-stdlib-uv1.js"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "date-fns": "^4.1.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "three": "^0.182.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.6",
    "patch-package": "^8.0.1",
    "postcss": "^8.4.49",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
