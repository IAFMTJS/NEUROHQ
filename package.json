{
  "name": "neurohq",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node -e \"console.log('\\n=== NEUROHQ dev server – watch THIS terminal for compilation (✓ Compiled) and errors (⨯ Error) ===\\n')\" && next dev -p 3000",
    "dev:clean": "node -e \"require('fs').rmSync('.next', { recursive: true, force: true }); console.log('Cleared .next cache. Starting dev...');\" && next dev -p 3000",
    "start": "next dev -p 3000",
    "build": "next build",
    "type-check": "tsc --noEmit",
    "deploy:check": "npm run type-check && next build",
    "serve": "next start -p 3000",
    "serve:prod": "next start -p 3001",
    "lint": "next lint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:visual:capture": "playwright test tests/visual/hud.spec.ts",
    "test:visual:diff": "node scripts/pixelmatch-hud.mjs",
    "test:visual": "npm run test:visual:capture && npm run test:visual:diff",
    "run:all": "npm run build && npm run test",
    "seed:quotes": "node scripts/seed-quotes.js",
    "build:icons": "node scripts/generate-pwa-icons.js",
    "generate-vapid": "node scripts/generate-vapid-keys.js",
    "db:types": "node scripts/gen-db-types.js",
    "postinstall": "patch-package && node scripts/patch-three-stdlib-uv1.js"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^5.2.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "date-fns": "^4.1.0",
    "geist": "^1.7.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "three": "^0.182.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^22.9.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/web-push": "^3.6.4",
    "@webgpu/types": "^0.1.69",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.6",
    "patch-package": "^8.0.1",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.49",
    "sharp": "^0.33.5",
    "ssim.js": "^3.5.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
